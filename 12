#include<stdio.h>
#define  Hash_Size  10
main()
{
    FILE   *fp;
    int i,key,hv,index,HT[Hash_Size];    
    for(i=0;i<Hash_Size;i++)
    HT[i]=-1;
    fp=fopen("1.txt","r");
    while(!feof(fp))
    {
        fscanf(fp,"%d ",&key);
        hv=key%Hash_Size;
        if(HT[hv]==-1)
        HT[hv]=key;
    else
     {
        printf("\n Collision for key %d:\n",key);
        for(i=0;i<Hash_Size;i++)
          {
              index=(hv+i)%Hash_Size;
              if(HT[index]==-1)     
                {
                   HT[index]=key;
                   printf("\n collision solved using linear probing\n");
                   break;
                 }
           }
       }
    }

    printf("adress\tkey\n");
    for(i=0;i<Hash_Size;i++)
    printf("%d\t%d\n",i,HT[i]);
}
